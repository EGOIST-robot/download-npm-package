{
  "name": "download-npm-package",
  "version": "3.1.4",
  "description": "Download a single npm package",
  "main": "dist/index.js",
  "bin": {
    "download-npm-package": "bin/cli.js"
  },
  "scripts": {
    "test": "nyc babel-tape-runner test.js && semistandard | snazzy",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "watch": "rm -rf dist && mkdir -p dist && babel -w lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kesla/download-npm-package.git"
  },
  "author": "David Bj√∂rklund <david.bjorklund@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kesla/download-npm-package/issues"
  },
  "homepage": "https://github.com/kesla/download-npm-package#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "co": "^4.6.0",
    "download-tarball": "^1.0.0",
    "get-package-json-from-registry": "^2.0.0",
    "home": "^1.0.1",
    "mkdirp-then": "^1.2.0",
    "npm-package-arg": "^4.2.0",
    "registry-auth-token": "^2.0.0",
    "registry-url": "^3.1.0",
    "rimraf-then": "^1.0.0",
    "then-fs": "^2.0.0",
    "tmp": "0.0.28"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-tape-runner": "^2.0.1",
    "http-shutdown": "^1.0.3",
    "nyc": "^7.0.0",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.0",
    "tapava": "^2.2.0",
    "tar-stream": "^1.5.2"
  }
}
